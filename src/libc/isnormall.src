	.assume	adl=1

	.section	.text
	
	.global	__isnormall
	.type	__isnormall, @function

; bool _isnormall(long double)
__isnormall:
	; load upper 24bits
	ld	hl, 8	
	add	hl, sp
	ld	hl, (hl)
	; clear signbit
	add	hl, hl
	; test for inf/NaN
	ld	de, $2000
	add	hl, de
	jr	c, .L.exp_all_ones
	; double de
	ld	d, $40
	; test for zero/subnormal
	sbc	hl, de
;	jr	c, .L.exp_all_zero

.L.exp_all_ones:
.L.exp_all_zero:
	sbc	a, a
	inc	a
	ret
