	.assume	adl=1

	.section	.text
	.global	_atof
	.global	_atoff
	.type	_atof, @function
	.type	_atoff, @function

.ifdef PREFER_OS_LIBC

	.set	_atoff, 0x022110
	.set	_atof, _atoff

.else

_atof:
_atoff:
	pop	hl
	pop	bc
	push	bc
	push	hl
	or	a,a
	sbc	hl,hl
	push	hl
	push	bc
	call	_strtod
	pop	bc
	pop	hl
	ret

	.extern	__frameset0
	.extern	_strtod

.endif
