; float _Complex CMPLXF(float x, float y) /* struct ABI */
_CMPLXF:
_CMPLX:
	pop	iy
	pop	de
	or	a, a
	sbc	hl, hl
	add	hl, sp
	push	de	; ZDS II sret
	ld	bc, 4	; sizeof(float _Complex)
