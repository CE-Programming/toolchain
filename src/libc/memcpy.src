	.assume	adl=1

	.section	.text

	.global	_memcpy
	.type	_memcpy, @function

.ifdef PREFER_CE_LIBC

	.set	_memcpy, 0x0000A4

.else

_memcpy:
	ld	iy, -1
	add	iy, sp
	ld	bc, (iy + 10)  ; Load count
	sbc	hl, hl
	add	hl, bc
	jr	nc, .L.zero
	ld	de, (iy + 4)  ; Load destination
	ld	hl, (iy + 7)  ; Load source
	ldir
.L.zero:
	ld	hl, (iy + 4)  ; Return the destination pointer
	ret

.endif
