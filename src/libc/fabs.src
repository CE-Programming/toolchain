	assume	adl=1

	section	.text
	public	_fabs, _fabsf, _fabsl

if PREFER_OS_LIBC

_fabsf := 0220F0h
_fabs := _fabsf

else

; CC: 2 + 8F + 4R + 1W
; 8 bytes
_fabs:
_fabsf:
	ld	hl, 6
	add	hl, sp
	res	7, (hl)
	ret

end if

; CC: 2 + 8F + 4R + 1W
; 8 bytes
_fabsl:
	ld	hl, 10
	add	hl, sp
	res	7, (hl)
	ret
