	.assume	adl=1

	.section	.text
	.global	_cos
	.type	_cos, @function
	.global	_cosf
	.type	_cosf, @function

.ifdef PREFER_OS_LIBC

	.set	_cosf, 0x02211C
	.set	_cos, _cosf

.else

; float _f32_sinus(int quad, float arg)
_cos:
_cosf:
	call	__frameset0
	ld	e, (ix + 9) ; exponent
	ld	hl, (ix + 6) ; mantissa
	res	7, e	; fabsf(x)
	push	de
	push	hl
	scf	; quad 1, N reset, C set
	push	af
	jp	_sinf.hijack

	.extern	__frameset0
	.extern	_sinf.hijack

.endif
