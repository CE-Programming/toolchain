	.assume	adl=1

	.section	.text
	.global	_tolower
	.type	_tolower, @function
	.global	_tolower.internal
	.type	_tolower.internal, @function

_tolower:
	pop	de
	ex	(sp), hl
	push	de
	; ASM interface: input/output char in L, destroys AF
_tolower.internal:
	ld	a, l
	sub	a, 'A'
	cp	a, 1+'Z'-'A'
	ret	nc

	set	5, l
	ret
