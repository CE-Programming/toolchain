	.assume	adl=1

	.section	.text
	.global	_ceil
	.global	_ceilf
	.type	_ceil, @function
	.type	_ceilf, @function

.ifdef PREFER_OS_LIBC

	.set	_ceilf, 0x022114
	.set	_ceil, _ceilf

.else

_ceil:
_ceilf:
	call	__frameset0
	ld	bc, (ix+6)
	ld	a, (ix+9)
	call	__fneg
	ld	l, a
	ld	h, 0
	push	hl
	push	bc
	call	_floor
	pop	bc
	pop	bc
	push	hl
	pop	bc
	ld	a, e
	call	__fneg
	ld	e, a
	push	bc
	pop	hl
	pop	ix
	ret

	.extern	__frameset0
	.extern	__fneg
	.extern	_floor

.endif
