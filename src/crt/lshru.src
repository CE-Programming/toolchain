	.assume	adl=1

	.section	.text
	.global	__lshru
	.type	__lshru, @function

.ifdef PREFER_CE_CRT

	.set	__lshru, 0x0001EC

.else

__lshru:
; Suboptimal for large shift amounts
; CC: if C!=0: C*(15*r(PC)+3*r(SPL)+3*w(SPL)+4)+17*r(PC)+9*r(SPL)+6*w(SPL)+2
;     if C==0: 4*r(PC)+3*r(SPL)+2
	inc	l
	dec	l
	ret	z
	push	hl
	push	bc
	ld	b, l
	or	a, a
	sbc	hl, hl
	add	hl, sp
.L.loop:
	srl	a
	jp	__lshrs.hijack_lshru

	.extern	__lshrs.hijack_lshru

.endif
