	.assume	adl=1

	.section	.text
	.global	__bshl
	.type	__bshl, @function

.ifdef PREFER_OS_CRT

	.set	__bshl, 0x000100

.else

__bshl:
; CC: if B!=0: B*(4*r(PC)+1)+6*r(PC)+(ADL?6*r(SPL)+3*w(SPL):4*r(SPS)+2*w(SPS))+1
;     if B==0: 4*r(PC)+(ADL?3*r(SPL):2*r(SPS))+2
	inc	b
	dec	b
	ret	z
	push	bc
.L.loop:
	add	a, a
	djnz	.L.loop
	pop	bc
	ret

.endif
