	.assume	adl=1

	.section	.text

	.global	__dtol
	.type	__dtol, @function
	.global	__dtoul
	.type	__dtoul, @function

; correctly handles all non-UB cases for both
; (long)long double and (unsigned long)long double
__dtol:
__dtoul:
	push	bc
	push	de
	call	__dtoll	; same as __dtoull
	ld	c, e
	pop	de
	ld	e, c
	pop	bc
	ret

	.extern	__dtoll
