	.assume	adl=1

	.section	.text
	.global	__ineg_fast
	.type	__ineg_fast, @function
	.global	__sneg_fast
	.type	__sneg_fast, @function

__ineg_fast:
; UHL = -UHL
;   CC: 7*r(PC)+3*r(SPL)+1
;   CC: 6 bytes | 7F + 3R + 1
	add	hl, de	; uhl=UHL+UDE
	ex	de, hl	; ude=UHL+UDE, uhl=UDE
	or	a, a
	sbc	hl, de	; uhl=UDE-(UHL+UDE)
			;    =UDE-UHL-UDE
			;    =-UHL
	ret

	.set	__sneg_fast, __ineg_fast
