; Performs 48-bit unsigned shift right
;
; Returns:
; ude:uhl = ude:uhl>>c

	assume	adl=1

	section	.text
	public	__i48shru
__i48shru:
	inc c
	dec c
	ret z
	push ix
	ld ix, 0
	add ix, sp
	push bc
	ld b, c
	push af
	push de
	push hl
	ld c, (ix-7)
	ld a, (ix-10)
	srl c
	jp __i48shrs.midloop
	
	extern __i48shrs.midloop