; Performs unsigned 48 bit shift right
; 
; Arguments:
; ude = Most significant bytes of arg 1
; uhl = Least significant bytes of arg 1
; c = Amount to shift by


; Returns:
; ude:uhl = ude:uhl>>c

	assume	adl=1

	section	.text
	public	__i48shru
__i48shru:
	inc c
	dec c
	ret z
	push bc
	ld b, c
	push de
	push hl
	ld hl, 5
	add hl, sp
	
	srl (hl)
	dec hl
	jp nz, __i48shrs.midloop
	extern __i48shrs.midloop