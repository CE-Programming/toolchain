	.assume	adl=1

	.section	.text
	.global	__lldivu_b
	.type	__lldivu_b, @function

__lldivu_b:
; Very slow
	push	af
	push	iy
	ld	iy, 0
	add	iy, sp
	ld	iy, (iy + 9)
	xor	a, a
	ld	iyh, 64
.L.loop:
	add	hl, hl
	ex	de, hl
	adc	hl, hl
	ex	de, hl
	rl	c
	rl	b
	rla
	jr	c, .L.1
	cp	a, iyl
	jr	c, .L.0
.L.1:
	sub	a, iyl
	inc	l
.L.0:
	dec	iyh
	jr	nz, .L.loop
	pop	iy
	pop	af
	ret
