; Performs 48-bit unsigned remainder
;
; Returns:
; ude:uhl = ude:uhl % uiy:ubc

	.assume	adl=1

	.section	.text
	.global	__i48remu
	.type	__i48remu, @function

__i48remu:
	push	iy
	push	bc

	call	__i48dvrmu
	lea	de, iy + 0
	or	a, a
	sbc	hl, hl
	add	hl, bc

	pop	bc
	pop	iy
	ret

	.extern	__i48dvrmu
