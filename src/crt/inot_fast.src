	.assume	adl=1

	.section	.text
	.global	__inot_fast
	.type	__inot_fast, @function
	.global	__snot_fast
	.type	__snot_fast, @function

__inot_fast:
; UHL = ~UHL
;   CC: 7*r(PC)+3*r(SPL)+1
;   CC: 6 bytes | 7F + 3R + 1
	add	hl, de	; uhl=UHL+UDE
	ex	de, hl	; ude=UHL+UDE, uhl=UDE
	scf
	sbc	hl, de	; uhl=UDE-(UHL+UDE)-1
			;    =UDE-UHL-UDE-1
			;    =-UHL-1
			;    =~UHL
	ret

	.set	__snot_fast, __inot_fast
