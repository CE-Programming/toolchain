	assume	adl=1

	section	.text
	public	__lnot

if PREFER_OS_CRT

__lnot := 0001D4h

else

__lnot:
	push	af
	ld	a, e	; a=E
	cpl		; a=~E
	ld	e, a	; e=~E
	pop	af
	push	de
	add	hl, de	; uhl=UHL+x
	ex	de, hl	; ude=UHL+x, uhl=x
	scf
	sbc	hl, de	; uhl=x-(UHL+x)-1
			;	=x-UHL-x-1
			;	=-UHL-1
			;	=~UHL
	pop	de
	ret

end if
