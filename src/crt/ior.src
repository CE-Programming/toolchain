	assume	adl=1

	section	.text
	public	__ior

if PREFER_OS_CRT

__ior := 000168h

else

__ior:
; CC: 24*r(PC)+11*r(SPL)+10*w(SPL)+1
	push	af
	public	__ior.hijack_lor
.hijack_lor:
	push	hl
	scf
	sbc	hl, hl
	add	hl, sp
	push	bc
	ld	a, (hl)
	inc	hl
	ld	sp, hl
	inc	hl
	inc	hl
	or	a, (hl)
	ld	(hl), a
	pop	hl
	ld	a, h
	or	a, b
	ld	h, a
	ld	a, l
	or	a, c
	ld	l, a
	pop	af
	ret

end if
