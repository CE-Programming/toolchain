	.assume	adl=1

	.section	.text
	.global	__lladd
	.type	__lladd, @function

__lladd:
	push	iy
	ld	iy, 0
	add	iy, sp
	push	bc
	ld	bc, (iy + 6)
	add	hl, bc
	ex	de, hl
	ld	bc, (iy + 9)
	adc	hl, bc
	ex	de, hl
	pop	bc
	jr	nc, .L.nc48
	inc	bc
.L.nc48:
	ld	iy, (iy + 12)
	add	iy, bc
	lea	bc, iy + 0
	pop	iy
	ret
