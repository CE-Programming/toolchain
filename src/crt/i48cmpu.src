; Performs 48-bit unsigned comparison
;
; Arguments:
; ude:uhl = left-hand side
; uiy:ubc = right-hand side
;
; Returns:
; flags updated according to comparison

	assume	adl=1

	section	.text
	public	__i48cmpu
__i48cmpu:
	push hl

	lea hl, iy+0 ;iy
	sbc hl, de
	jq nz, .ccfExit
	pop hl
	push hl
	sbc hl, bc ; subtracting operands the other way around
	pop hl
	ret ; don't compliment carry
.ccfExit:
	pop hl
	ccf
	ret