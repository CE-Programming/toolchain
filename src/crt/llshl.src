	.assume	adl=1

	.section	.text
	.global	__llshl
	.type	__llshl, @function

__llshl:
; Suboptimal for large shift amounts
	push	af
	ld	a, b

	push	hl
	ld	hl, 9
	add	hl, sp
	ld	b, (hl)
	pop	hl

	inc	b
	jr	.L.begin

.L.loop:
	add	hl, hl
	ex	de, hl
	adc	hl, hl
	ex	de, hl
	rl	c
	rla
.L.begin:
	djnz	.L.loop

	ld	b, a
	pop	af
	ret
