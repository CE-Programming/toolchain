	.assume	adl=1

	.section	.text

	.global	__drem
	.type	__drem, @function

; float64_t f64_rem(bool, float64_t, const float64_t*)
__drem:
	push af
push iy
	ld	iy, 9
	add	iy, sp
	push iy
	push bc
	push de
	push hl
	
	rl	b
	push	af	; Carry = signbit(x)

	call	___f64_rem

	pop af
	pop af
	pop af
	pop af
	pop af
	pop iy
	pop af	; restore
	ret

	.extern	___f64_rem
