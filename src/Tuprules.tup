.gitignore

ezcflags += -ffreestanding
ezcflags += -Wall -Wextra
ezcflags += -Oz
ezcflags += -mllvm -profile-guided-section-prefix=false
ezcppflags += $(ezcflags)
ezcppflags += -fno-exceptions -fno-rtti
!ezcc = |> ^ ezcc %b^ @(EZCC) -S $(ez%eflags) %f -o %o |> build/%f.src

incdir = $(TUP_CWD)/include
!fasmg = | $(incdir)/* ^\\.gitignore |> ^ fasmg %b^ @(FASMG) -n %f %o |> %B.8xv | %B.lib ../<libs>
